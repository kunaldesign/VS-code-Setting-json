		"file-loader": "^6.2.0",
		"glob": "^7.1.7",
		"httpsnippet": "^2.0.0",
		"jsonc-parser": "^3.0.0",
		"mocha": "^9.1.3",
		"node-fetch": "2",
		"npm-run-all": "^4.1.5",
		"quicktype-core": "^6.0.70",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"stringify-object": "^4.0.1",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.4",
		"uuid": "^8.3.2",
		"vscode-nls": "^5.0.0",
		"web-vitals": "^2.1.4",
		"webpack": "^5.52.1",
